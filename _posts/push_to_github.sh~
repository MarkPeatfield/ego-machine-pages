#!/bin/bash

#this is intended to publish the progress graph to the blog

# Define the repository path
repo_path="/Users/markpeatfield/Darkness-and-Light/"

# Change directory to the repository
cd $repo_path

# Add all files to the repository
git add .

# Commit the changes
git commit -m "Commit changes"

# Push the changes to the remote repository
git push https://github.com/MarkPeatfield/Darkness-and-Light.git
# Display successful message
echo "Push to remote repository completed"
